{
  "name": "accelerator-first-project",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://tsbv.github.io/life-tour/",
  "scripts": {
    "dev": "vite --host --open",
    "test": "backstop test --config=pp.config.cjs",
    "test-content": "vitest content.test.ts --ui",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "convert-rastr": "node convert-rastr.js",
    "w3c": "html-validator source --quiet",
    "linthtml": "linthtml source",
    "html-validate": "html-validate source/index.html",
    "lint-bem": "node bem-tree.js",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --fix",
    "lint-js": "eslint source/js --fix",
    "ls-lint": "ls-lint",
    "editorconfig": "editorconfig-cli",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "3.0.0",
    "@linthtml/linthtml": "0.10.1",
    "@ls-lint/ls-lint": "2.2.3",
    "@spiriit/vite-plugin-svg-spritemap": "2.3.1",
    "@vitest/ui": "2.1.3",
    "autoprefixer": "10.4.20",
    "cli-progress": "3.12.0",
    "eslint": "9.13.0",
    "eslint-config-htmlacademy": "10.0.1",
    "gh-pages": "^6.2.0",
    "glob": "11.0.0",
    "gulp-html-bemlinter": "4.0.0",
    "html-validate": "8.24.2",
    "htmlacademy-backstopjs": "6.6.1",
    "linthtml-config-htmlacademy": "1.0.25",
    "sass": "1.80.3",
    "sharp": "0.33.5",
    "stylelint": "16.10.0",
    "stylelint-config-htmlacademy": "4.2.3",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-selector-bem-pattern": "4.0.1",
    "svgo": "3.3.2",
    "vinyl-fs": "4.0.0",
    "vite": "5.4.9",
    "vite-plugin-image-optimizer": "1.1.8",
    "vite-plugin-minify": "2.0.0",
    "vitest": "^2.1.3",
    "w3c-html-validator": "1.8.2"
  },
  "dependencies": {
    "swiper": "11.1.14"
  },
  "engines": {
    "node": "^18.18 || ^20.9"
  },
  "packageManager": "pnpm@9.12.2"
}
